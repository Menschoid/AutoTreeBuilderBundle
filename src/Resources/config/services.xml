<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="menschoid_auto_tree_builder.parameter_processor" class="Menschoid\AutoTreeBuilderBundle\ParameterProcessor" public="true" />
        <service id="Menschoid\AutoTreeBuilderBundle\ParameterProcessor" alias="menschoid_auto_tree_builder.parameter_processor" />

        <service id="menschoid_auto_tree_builder.tree_builder_provider" class="Menschoid\AutoTreeBuilderBundle\TreeBuilderProvider" public="true" />
        <service id="Menschoid\AutoTreeBuilderBundle\TreeBuilderProvider" alias="menschoid_auto_tree_builder.tree_builder_provider" />

        <service id="menschoid_auto_tree_builder.config_parameter" class="Menschoid\AutoTreeBuilderBundle\Attributes\ConfigParameter" public="true" />
        <service id="Menschoid\AutoTreeBuilderBundle\Attributes\ConfigParameter" alias="menschoid_auto_tree_builder.config_parameter" />
    </services>
</container>